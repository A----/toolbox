{
    "name": "rockettheme/toolbox",
    "type": "library",
    "description": "RocketTheme Toolbox Library",
    "keywords": ["rockettheme", "php"],
    "homepage": "http://www.rockettheme.com",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "ircmaxell/password-compat": "1.0.*",
        "pimple/pimple": "~3.0",
        "symfony/yaml": "~2.5",
        "symfony/event-dispatcher": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "RocketTheme\\Toolbox\\ArrayTraits\\": "RocketTheme/Toolbox/ArrayTraits/src",
            "RocketTheme\\Toolbox\\Blueprints\\": "RocketTheme/Toolbox/Blueprints/src",
            "RocketTheme\\Toolbox\\Config\\": "RocketTheme/Toolbox/Config/src",
            "RocketTheme\\Toolbox\\Data\\": "RocketTheme/Toolbox/Data/src",
            "RocketTheme\\Toolbox\\DI\\": "RocketTheme/Toolbox/DI/src",
            "RocketTheme\\Toolbox\\EventDispatcher\\": "RocketTheme/Toolbox/EventDispatcher/src",
            "RocketTheme\\Toolbox\\Filesystem\\": "RocketTheme/Toolbox/Filesystem/src",
            "RocketTheme\\Toolbox\\Session\\": "RocketTheme/Toolbox/Session/src"
        }
    }
}
